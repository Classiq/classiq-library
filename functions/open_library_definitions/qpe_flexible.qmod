qfunc qpe_flexible<unitary_with_power: qfunc <arg0: int>()>(phase: qnum) {
  phase_array: qbit[];
  phase -> phase_array;
  apply_to_all<H>(phase_array);
  repeat (index: phase_array.len) {
    control (phase_array[index]) {
      unitary_with_power<2 ** index>();
    }
  }
  invert {
    qft(phase_array);
  }
  phase_array -> phase;
}
