qfunc main(output nodes: qbit[], angles: real[6]) {
  allocate(4, nodes);
  hadamard_transform(nodes);
  single_pauli_exponent([
    Pauli::Y,
    Pauli::Z,
    Pauli::I,
    Pauli::I
  ], angles[0], nodes);
  single_pauli_exponent([
    Pauli::I,
    Pauli::Z,
    Pauli::Y,
    Pauli::I
  ], angles[1], nodes);
  single_pauli_exponent([
    Pauli::I,
    Pauli::Z,
    Pauli::I,
    Pauli::Y
  ], angles[2], nodes);
  single_pauli_exponent([
    Pauli::Z,
    Pauli::I,
    Pauli::Y,
    Pauli::I
  ], angles[3], nodes);
  single_pauli_exponent([
    Pauli::Z,
    Pauli::I,
    Pauli::I,
    Pauli::Y
  ], angles[4], nodes);
  single_pauli_exponent([
    Pauli::I,
    Pauli::I,
    Pauli::Z,
    Pauli::Y
  ], angles[5], nodes);
}
